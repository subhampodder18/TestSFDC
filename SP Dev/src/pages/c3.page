<apex:page controller="c3">
  <apex:form >
  <apex:pageBlock title="Make a New Account" >
      <apex:pageBlockSection >
          <apex:inputField value="{!accnt.Name}" label="Name"/> <br/>
          <apex:inputField value="{!accnt.Industry}"/> <br/>
          <apex:inputField value="{!accnt.Type}"/>
      </apex:pageBlockSection>
     <apex:pageBlockButtons >
         <apex:commandButton value="Save" action="{!save}"/>
     </apex:pageBlockButtons>
  </apex:pageBlock>
      
 </apex:form>
</apex:page>